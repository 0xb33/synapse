worker_app: "{{ app }}"
worker_name: "{{ name }}"

# The replication listener on the main synapse process.
worker_replication_host: 127.0.0.1
worker_replication_http_port: 9093

worker_listeners:
  - type: http
    port: {{ port }}
    resources:
      - names:
{%- for resource in listener_resources %}
        - {{ resource }}
{%- endfor %}
